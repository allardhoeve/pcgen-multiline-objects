  <build>
    <nagEmailAddress>jdempsey@users.sourceforge.net</nagEmailAddress>
    <sourceDirectory>code/src/java</sourceDirectory>
    <unitTestSourceDirectory>code/src/test</unitTestSourceDirectory>
    <unitTest>
      <includes>
        <include>**/*Test.java</include>
      </includes>
    </unitTest>
    <resources>
      <resource>
        <!-- The resources are in the same place as the sources. -->
        <directory>code/src/java</directory>
        <includes>
          <include>**/*.properties</include>
        </includes>
      </resource>
      <resource>
        <!-- The resources are in the same place as the sources. -->
        <directory>code/src/java</directory>
        <includes>
          <include>pcgen/gui/resource/*</include>
        </includes>
      </resource>
    </resources>
  </build>
